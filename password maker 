print("your password is chracters at the index number that you enter thry must be 4 digit : ")

words = "gjfhfhvvg"
print(words)
password = input("enter :")
ogpass =''.join(words[int(i)] for i in password)

apass = input("enter:") 
if apass == ogpass :
	print("unlock")
if apass != ogpass :
	print("no unlock")
	
